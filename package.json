      "require": "./dist/state-machines.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && vite build"
  },
  "devDependencies": {
    "@types/node": "^18.11.10",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vite-plugin-dts": "^1.7.1"
  },
  "dependencies": {
    "@sniptt/monads": "^0.5.10"
  }
}
