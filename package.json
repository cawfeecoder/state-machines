{
  "name": "state-machines",
  "private": false,
  "version": "0.0.1",
  "description": "A simple, predictable state machine library for TypeScript",
  "author": "Nicholas Frush <nick@frushco.de>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/cawfeecoder/state-machines.git"
  },
  "bugs": {
    "url": "https://github.com/cawfeecoder/state-machines/issues"
  },
  "keywords": [
    "state",
    "machine",
    "state-machine",
    "fsm"
  ],
  "type": "module",
  "main": "./dist/state-machines.umd.cjs",
  "module": "./dist/state-machines.js",
  "types": "./dist/index.d.ts",
  "exports": {
